{
  "name": "serial-calendar",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.6.3",
  "pnpm": {
    "onlyBuiltDependencies": ["esbuild"]
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "run-s --continue-on-error lint:biome",
